<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lind.mybatis.dao.UserDao">
    <select id="queryUsers" resultType="com.lind.mybatis.entity.TUser">
        select username
        from t_user
        order by id
    </select>
</mapper>
